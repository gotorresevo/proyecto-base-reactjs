{
    "env": {
        "node": true,
        "browser": true,
        "es2020": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {},
    "settings": {
        "react": {
            "pragma": "React",
            "version": "^16.13.1"
        }
    },
    "overrides": [
        {
          "files": [
            "**/*.test.js",
            "**/*.spec.jsx"
          ],
          "env": {
            "jest": true
          }
        }
      ]
}